{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "resolveJsonModule": true,
        "sourceMap": true,
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules"
    ],
    "lib": [
        "es2016"
    ]
}